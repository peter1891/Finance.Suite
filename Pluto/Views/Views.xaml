<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:Pluto.Views"
                    xmlns:vm="clr-namespace:Finance.ViewModels;assembly=Finance.ViewModels">

    <DataTemplate DataType="{x:Type TypeName=vm:AccountsViewModel}">
        <v:AccountsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:AllocationsViewModel}">
        <v:AllocationsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:DialogViewModel}">
        <v:DialogView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:DashboardViewModel}">
        <v:DashboardView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:FormViewModel}">
        <v:FormView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:LoginViewModel}">
        <v:LoginView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:TransactionsViewModel}">
        <v:TransactionsView/>
    </DataTemplate>

</ResourceDictionary>