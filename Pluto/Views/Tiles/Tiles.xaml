<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:Pluto.Views.Tiles"
                    xmlns:vm="clr-namespace:Finance.ViewModels.Tiles;assembly=Finance.ViewModels">

    <DataTemplate DataType="{x:Type TypeName=vm:AllocationsTileViewModel}">
        <v:AllocationsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:ExpensesTileViewModel}">
        <v:ExpensesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:IncomeTileViewModel}">
        <v:IncomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:TotalBalanceTileViewModel}">
        <v:TotalBalanceView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:TransactionsTileViewModel}">
        <v:TransactionsView/>
    </DataTemplate>

</ResourceDictionary>