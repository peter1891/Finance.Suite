<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:Pluto.Views.Tiles"
                    xmlns:vm="clr-namespace:Finance.Factory.TileFactory.Tiles;assembly=Finance.Factory">

    <DataTemplate DataType="{x:Type TypeName=vm:AllocationsTile}">
        <v:AllocationsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:IncomeExpensesTile}">
        <v:IncomeExpensesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:TotalBalanceTile}">
        <v:TotalBalanceView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:TransactionsTile}">
        <v:TransactionsView/>
    </DataTemplate>

</ResourceDictionary>